{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "pwa-node",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ] 
    }

  ]
}
